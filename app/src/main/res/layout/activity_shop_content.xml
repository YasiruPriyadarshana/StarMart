<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".shop_content">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="411dp"
        android:layout_height="199dp"
        app:srcCompat="@android:color/holo_orange_light"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="6dp" />

    <HorizontalScrollView
        android:layout_width="403dp"
        android:layout_height="511dp"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="201dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical" />
    </HorizontalScrollView>

    <fragment
        android:id="@+id/shop_frag"
        android:name="com.wonder.starmart.ShopFragment1"
        android:layout_width="407dp"
        android:layout_height="401dp"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="205dp"></fragment>

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="363dp"
        android:layout_height="68dp"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="606dp" />
</androidx.constraintlayout.widget.ConstraintLayout>